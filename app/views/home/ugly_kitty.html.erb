<div class="container" >
	<div class="d-flex justify-content-center"><img style="width:25vmax;" src="https://img.cryptokitties.co/0x06012c8cf97bead5deae237070f9587f8e7a266d/<%= @kitty.kitty_id%>.svg"></div>
	<div class="d-flex justify-content-center"><h1><%= @kitty.kitty_name %></h1></div>
	<br>
	<hr>
	<% @kitty.q_a.each do |key, value| %>
		<nav aria-label="breadcrumb">
		  <ol class="breadcrumb">
		    <li class="breadcrumb-item active" aria-current="page"><strong>Question: </strong><%= value['q'] %></li>
		  </ol>
		</nav>
		<hr>
		<nav aria-label="breadcrumb">
		  <ol class="breadcrumb" style="background-color:white !important;">
		    <li class="breadcrumb-item active" aria-current="page"><strong>Answer: </strong><%= value['a'] %></li>
		  </ol>
		</nav>
	<% end %>
</div>